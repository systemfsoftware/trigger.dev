{
  "name": "@systemfsoftware/trigger.dev_openai",
  "version": "3.0.0-beta.55",
  "description": "The official OpenAI integration for Trigger.dev",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@systemfsoftware/trigger.dev_tsconfig": "workspace:*",
    "@types/node": "^20.16.2",
    "rimraf": "^3.0.2",
    "tsup": "8.0.1",
    "@systemfsoftware/trigger.dev_tsup": "workspace:*",
    "typescript": "5.5.4",
    "@types/jest": "^29.5.3",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:tsup",
    "build:tsup": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "jest"
  },
  "dependencies": {
    "openai": "^4.16.1",
    "@systemfsoftware/trigger.dev_sdk": "workspace:^3.0.0-beta.55",
    "@systemfsoftware/trigger.dev_integration-kit": "workspace:^3.0.0-beta.55"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
